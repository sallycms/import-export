{
    "name": "sallycms/import-export",
    "description": "AddOn for SallyCMS, providing file and database backup and restore capabilities",
    "authors": [
        {
            "name": "webvariants GbR"
        }
    ],
    "homepage": "http://www.sallycms.de/",
    "license": "MIT",
    "type": "sallycms-addon",
    "require": {
        "php": ">=5.2.0",
        "sallycms/composer-installer": "~1.1",
        "sallycms/sallycms": ">=0.9,<0.11"
    },
    "autoload": {
        "psr-0": {
            "sly_Controller_": "lib/",
            "sly\\ImportExport\\": "lib/",
            "": "lib/vendor/"
        }
    },
    "extra": {
        "sallycms": {
            "version": "4.x-dev"
        },
        "branch-alias": {
            "dev-default": "4.x-dev"
        }
    }
}
